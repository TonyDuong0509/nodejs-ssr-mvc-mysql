<!-- Paging -->
<ul class="pagination pull-right">
  <% if(page > 1) {%>
  <li>
    <a href="javascript:void(0)" onclick="goToPage(Number(page) - 1)"
      >&laquo;</a
    >
  </li>
  <% } %> <% for(let i = 1; i <= totalPage; i++) {%>
  <li class="<%= Number(page) == i ? 'active' : '' %>">
    <a href="javascript:void(0)" onclick="goToPage('<%= i %>')"><%= i %></a>
  </li>
  <% } %> <% if(page < totalPage) {%>
  <li>
    <a href="javascript:void(0)" onclick="goToPage(Number(page) + 1)"
      >&raquo;</a
    >
  </li>
  <% } %>
</ul>
<!-- End paging -->
